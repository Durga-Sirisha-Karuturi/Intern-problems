import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    int n,i,j,arr[],k;
	    Scanner input=new Scanner(System.in);
	    n=input.nextInt();
	    arr=new int[n];
	    for(i=0;i<n;i++){
	        arr[i]=input.nextInt();
	    }
	     int mid=n/2;
	    if(n%2!=0){
	         System.out.println("Median = "+arr[mid]);
	    }
	    else{
	        double avg=((double)arr[mid]+(double)arr[mid-1])/2;
	        System.out.println("Median = "+avg);
	        
	    }
	}
}
