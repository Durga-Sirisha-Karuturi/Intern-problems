#include<stdio.h>
int main()
{
   int a[1000], n, element, pos=0;
   int i;

   printf("Enter number of array elements:");
   scanf("%d", &n);

   printf("Enter array elements: ");
   for(i=0; i<n; i++)
   scanf("%d", &a[i]);

   printf("Enter element to search: ");
   scanf("%d",&element);

   for(i=0; i<n; i++)
   {
     if(a[i]==element)
     {
       printf("%d found at position %d", element, i+1);
       
     }
   }

   printf("%d not found.", element);
   
}
